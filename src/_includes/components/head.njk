<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {% set displayTitle = renderData.title or title or metadata.title%}
  <title>{{  displayTitle | prefixTitle }}</title>
  <link rel="icon" type="image/png" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 100 100%22><text y=%22.9em%22 font-size=%2290%22>🐶</text></svg>">
  <meta name="description" content="{{ metaDescription or renderData.metaDescription or metadata.description }}">
  {% set css %}
  {% include "assets/css/turret.css" %}
  {% include "assets/css/pandaSyntax.css" %}
  {% endset %}
  <style>
    {{css | cssmin | safe}}
  </style>
  {% set js %}
  {% include "assets/js/inline.js" %}
  {% endset %}
  <script>
    {{ js | jsmin | safe }}
  </script>
</head>
